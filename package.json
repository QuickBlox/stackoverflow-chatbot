  },
  "homepage": "https://github.com/soulfly/stackoverflow_tags_trigger-slack",
  "dependencies": {
    "backoff": "^2.5.0",
    "node-cron": "^1.2.0",
    "quickblox": "^2.6.0",
    "rimraf": "^2.6.1",
    "stackexchange": "^0.4.0",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1"
  },
  "babel": {
    "presets": [
      [
        "env",
